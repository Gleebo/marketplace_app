<h1>Your Listings</h1>

<ul>
  <% current_user.listings.each do |listing| %>
    <h2><%= listing.title %></h2>
    <p><%= listing.description %></p>
    <div><%= listing.price %></div>
    <%= link_to 'Edit', edit_listing_path(listing) %>
    <%= link_to 'Destroy', listing, method: :delete, data: { confirm: 'Are you sure?' } %>
  <% end %>
</ul>